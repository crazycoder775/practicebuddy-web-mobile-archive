{
  "name": "nativescript-audio",
  "version": "3.0.0",
  "description": "NativeScript plugin to record and play audio.",
  "main": "audio",
  "typings": "index.d.ts",
  "nativescript": {
    "platforms": {
      "android": "2.3.0",
      "ios": "2.3.0"
    }
  },
  "scripts": {
    "build": "tsc",
    "demo.ios": "npm run preparedemo; cd demo; tns emulate ios",
    "demo.android": "npm run preparedemo; cd demo; tns emulate android",
    "preparedemo": "npm run build; cd demo; tns plugin remove nativescript-audio; tns plugin add ..; tns install",
    "setup": "cd demo; npm i; cd ..; npm i; npm run build; cd demo; tns plugin add ..; cd .."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bradmartin/nativescript-audio.git"
